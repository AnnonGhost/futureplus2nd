{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run vercel-build",
  "outputDirectory": ".next",
  "installCommand": "npm install && npm run postinstall",
  "functions": {
    "app/**/*.{js,ts,tsx}": {
      "maxDuration": 30
    },
    "pages/api/**/*.{js,ts}": {
      "maxDuration": 30
    }
  },
  "crons": []
}